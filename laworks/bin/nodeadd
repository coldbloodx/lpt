#!/usr/bin/env python

import sys
import signal
import time
import os

from syslog_listener import sig_handler, SyslogListener

SYSLOG = "/var/log/syslog"
TESTLOG = "log.log"

def main():
    signal.signal(signal.SIGINT, sig_handler)
    listener = SyslogListener('log.log')
    while(True):
        mac = listener.listen().next()
        if not mac:
            continue
        print mac

if __name__ == '__main__': main()

